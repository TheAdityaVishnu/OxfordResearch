k=1;

for m = 1:3

folder = Names{m};
audio_files = dir(fullfile(folder));
audio_files(1,:) =[];
audio_files(1,:) =[];

no_of_sub_files = size(audio_files,1);

for i = 1:no_of_sub_files

List{k,1} = audio_files(i).name;

if(A(m) == 1)
List{k,2} = 'male';
else
List{k,2} = 'female';
end
k = k + 1;

end

end
